<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.util.ming.projectutil">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name="com.util.ming.projectutil.application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".demo.mvp.MVPActivity"
            android:label="MVPdemo"
            android:launchMode="singleTop">

        </activity>
        <activity
            android:name=".demo.rxjava.RxJavaActivity"
            android:label="RxJavademo"
            android:launchMode="singleTop">

        </activity>
        <activity
            android:name=".demo.fresco.FrescoActivity"
            android:label="Frescodemo"
            android:launchMode="singleTop">

        </activity>
        <activity
            android:name=".demo.autofocus.AutoFocusActivity"
            android:label="AutoFocusActivity"
            android:launchMode="singleTop">

        </activity>
        <activity
            android:name=".demo.retrofitdemo.Retrofit2Activity"
            android:label="Retrofit2Activity"
            android:launchMode="singleTop">

        </activity>
    </application>

</manifest>